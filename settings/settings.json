{
  "env": {
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "16384",
    "BASH_DEFAULT_TIMEOUT_MS": "30000",
    "NODE_ENV": "development"
  },
  "permissions": {
    "allow": [
      "Read",
      "Write",
      "Edit",
      "Bash(git:*)",
      "Bash(npm:*)",
      "Bash(npm run:*)",
      "Bash(npx:*)",
      "Bash(yarn:*)",
      "Bash(pnpm:*)",
      "Bash(cargo:*)",
      "Bash(pip:*)",
      "Bash(pip3:*)",
      "Bash(poetry:*)",
      "Bash(python:*)",
      "Bash(python3:*)",
      "Bash(node:*)",
      "Bash(tsc:*)",
      "Bash(tsx:*)",
      "Bash(bun:*)",
      "Bash(jest:*)",
      "Bash(vitest:*)",
      "Bash(pytest:*)",
      "Bash(eslint:*)",
      "Bash(prettier:*)",
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(grep:*)",
      "Bash(find:*)",
      "Bash(wc:*)",
      "Bash(diff:*)",
      "Bash(mkdir:*)",
      "Bash(touch:*)",
      "Bash(cp:*)",
      "Bash(mv:*)",
      "Bash(echo:*)",
      "Bash(which:*)",
      "Bash(pwd)",
      "Bash(date:*)",
      "Bash(service:*)",
      "Bash(docker:*)",
      "Bash(docker-compose:*)",
      "Bash(make:*)",
      "Bash(jq:*)",
      "Bash(sed:*)",
      "Bash(awk:*)",
      "Bash(sort:*)",
      "Bash(uniq:*)",
      "Bash(xargs:*)",
      "Bash(curl -O:*)",
      "Bash(curl -L:*)",
      "Bash(wget -O:*)"
    ],
    "deny": [
      "Read(.env*)",
      "Read(*.pem)",
      "Read(*.key)",
      "Read(.npmrc)",
      "Read(.pypirc)",
      "Read(~/.ssh/**)",
      "Read(~/.aws/**)",
      "Read(~/.config/gh/**)",
      "Read(~/.netrc)",
      "Read(~/.git-credentials)",
      "Write(/etc/**)",
      "Write(/usr/**)",
      "Write(/var/**)",
      "Write(/System/**)",
      "Write(/Library/**)",
      "Write(~/.ssh/**)",
      "Write(~/.aws/**)",
      "Write(~/.zshrc)",
      "Write(~/.bashrc)",
      "Write(~/.bash_profile)",
      "Bash(rm -rf /)",
      "Bash(rm -rf ~)",
      "Bash(rm -rf /:*)",
      "Bash(rm -rf ~/:*)",
      "Bash(rm -rf .)",
      "Bash(rm -rf ..)",
      "Bash(sudo:*)",
      "Bash(chmod -R 777:*)",
      "Bash(chown:*)",
      "Bash(mkfs:*)",
      "Bash(dd:*)",
      "Bash(shutdown:*)",
      "Bash(reboot:*)",
      "Bash(killall:*)",
      "Bash(pkill:*)",
      "Bash(curl -X POST:*)",
      "Bash(curl --data:*)",
      "Bash(curl -d:*)",
      "Bash(scp:*)",
      "Bash(rsync:*)",
      "Bash(nc:*)",
      "Bash(netcat:*)",
      "Bash(ncat:*)",
      "Bash(sftp:*)",
      "Bash(ftp:*)",
      "Bash(security:*)",
      "Bash(osascript:*)",
      "Bash(open -a:*)",
      "Bash(eval:*)",
      "Bash(exec:*)",
      "Bash(source ~/:*)",
      "Bash(. ~/:*)"
    ]
  },
  "model": "opus",
  "statusLine": {
    "type": "command",
    "command": "bash -c 'node \"$(ls -td ~/.claude/plugins/cache/claude-hud/claude-hud/*/ 2>/dev/null | head -1)dist/index.js\"'"
  },
  "enabledPlugins": {
    "claude-hud@claude-hud": true,
    "claude-mem@thedotmack": true,
    "frontend-design@claude-plugins-official": true
  }
}